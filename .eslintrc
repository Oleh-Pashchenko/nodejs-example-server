{
    "env": {
        "es6": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4],
        "no-unneeded-ternary":"off",
        "no-underscore-dangle":"off",
        "import/no-dynamic-require": "off",
        "global-require": "off",
        "import/no-unresolved": "off",
        "no-return-await": 0
    }
}