{
  "name": "nodejs-postgres-example-server",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint ./*.js ./**/*.js",
    "lint:fix": "node ./node_modules/.bin/eslint --fix ./*.js ./**/*.js",
    "start": "npm run migrate-up & ./node_modules/.bin/pm2-runtime start ecosystem.config.js --watch",
    "start:demon": "./node_modules/.bin/pm2 start ecosystem.config.js",
    "stop": "./node_modules/.bin/pm2 stop config/ecosystem.config.js",
    "test": "node node_modules/.bin/jest test --watch --coverage",
    "migrate": "./node_modules/.bin/sequelize migration:generate --name ${name}",
    "migrate-up": "./node_modules/.bin/sequelize db:migrate",
    "migrate-down": "./node_modules/.bin/sequelize db:migrate:undo"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "http-status-codes": "^1.1.6",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.8.2",
    "pg": "^7.8.0",
    "pm2": "^3.2.2",
    "redis": "^2.8.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "swagger-ui-express": "^4.0.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-http": "^4.2.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^20.0.4"
  }
}
